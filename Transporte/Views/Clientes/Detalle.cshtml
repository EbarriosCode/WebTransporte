@model Logic.clientes
@{
    ViewBag.Title = "Detalle";
    Layout = "~/Views/Shared/_LayoutLogueados.cshtml";
}
<div class="container">
    <br />
    <div class="jumbotron">
        <h2 class="text-center">Detalle del Cliente</h2>
    </div>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Clientes")">Regresar</a></li>
        <li class="active"><a href="#">@(Model.nombre+" "+@Model.apellido)</a></li>
    </ol>

    <div class="panel panel-default">
        <div class="panel-heading"><strong>Información Personal</strong></div>
        <div class="panel-body">
            <dl class="dl-horizontal">
                <dt>Nombres</dt>
                <dd>@Model.nombre</dd>
                <dt>Apellidos</dt>
                <dd>@Model.apellido</dd>
                <dt>Dpi</dt>
                <dd>@Model.dpi</dd>
                <dt>Nit</dt>
                <dd>@Model.nit</dd>
                <dt>Teléfono</dt>
                <dd>@Model.telefono</dd>
                <dt>Dirección</dt>
                <dd>@Model.direccion</dd>
                <dt>Departamento</dt>
                <dd>@Model.departamento</dd>
                @*<dd>@Model.departamento1.nombreDepartamento</dd>*@
            </dl>
        </div>
    </div>
</div>

